<div id="main" style="display:{{vista}}">

<!-- Leasing -->
<div id="leasing">
	<div style="display:block;height: 2px;background-color: #9aa5ad;"></div>
	<!--<div class="field" style="background-color:#f5f5f5;">No viable </div>-->
	<div class="field field--bool">
		<label class="field__label">No viable Leasing</label>
		<div class="field__controls">
			<input class="checkbox_default" type="checkbox" id="checkbox_no_viable">
		</div>
	</div>
	<div class="field field--enum hide noViable">
		<label class="field__label">Razón de Lead no viable</label>
		<div class="field__controls">
			<select name="enum" id="razon_nv_leasing">
				{{#each razones_ddw_list}}
				<option value="{{@key}}">{{this}}</option>
				{{/each}}
			</select>
			<i class="icondefault icon icon-caret-down selectArrow-icon"></i>
		</div>
	</div>

	<!-- Fuera de Perfil (Razón)-->
	<div class="field field--enum hide noViable">
		<label class="field__label">Fuera de Perfil (Razón)</label>
		<div class="field__controls">
			<select name="enum" id="fuera_perfil_razon">
				{{#each fuera_de_perfil_ddw_list}}
				<option value="{{@key}}">{{this}}</option>
				{{/each}}
			</select>
			<i class="icondefault icon icon-caret-down selectArrow-icon"></i>
		</div>
	</div>
	<!--fin Fuera de Perfil (Razón)-->

	<!-- Condiciones financieras-->
